{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf .cache/redux/;npx gatsby build",
    "develop": "rm -rf .cache/redux/;npx gatsby develop",
    "create-dummy": "ruby ./script/create_dummy.rb > ./hatena-group-exports/dummy.mt",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "npx gatsby serve",
    "clean": "npx gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "escape-goat": "^3.0.0",
    "gatsby": "^2.19.45",
    "gatsby-plugin-lunr": "^1.5.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "install": "^0.13.0",
    "lunr-languages": "^1.4.0",
    "mt-parser": "^1.0.0-alpha.6",
    "node-html-parser": "^1.2.13",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
